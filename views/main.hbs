        function initScroll() {
            $('.bloomies-store-full-image').attr('style', '--transition-speed: 1s');

            w.on('scroll', function() {
                var windowScrollTop = w.scrollTop();
                var windowCenter = windowScrollTop + (w.height() / 2);

                // set the store image parallax
                if(windowScrollTop < w.height()) {
                    var storePanel = $('.bloomies-store-panel');
                    var storeCenter = storePanel.offset().top + (storePanel.height() / 2);
                    var storeDist = storeCenter - windowCenter;
                    var storeTransY = storeDist * .15;
                    $('.bloomies-store-full-image').css({'transform' : 'translateY(' + storeTransY + 'px)'});
                }
            });

            $('.scroll-div').each(function() {
                $(this).find('.grid-item').each(function() {
                    $(this).data('offset-num', parseInt($(this).data('offset')));
                    $(this).data('speed-num', parseInt($(this).data('speed')));
                    $(this).data('down-speed-num', parseInt($(this).data('down-speed')));
                });
            });


            setInterval(function() {
                var windowCenter = w.scrollTop() + (w.height() / 2); 
                var windowScrollTop = w.scrollTop();
                var a = 0;
                
                $('.scroll-div').each(function() {
                    var container = $(this);
                    var sectionCenter = container.offset().top + (container.height() / 2);
                    var sectionDist = sectionCenter - windowCenter;

                    container.find('.grid-item').each(function() {
                        var g = $(this);
                        var targetTransY = sectionDist;

                        if(a > 0) {
                            targetTransY = sectionDist * 1.2;
                        }

                        var currentTransY = parseInt(g.css('transform').split(',')[5]);
                        var speed;
                        currentTransY < targetTransY ? speed = g.data('down-speed-num') : speed = g.data('speed-num');
                        speed *= .5;
                        var transYDif = targetTransY - currentTransY;
                        g.css({'transform' : 'translateY(' + (currentTransY + (transYDif/speed)) + 'px)'});
                    });

                    a += 1;
                });
            }, 40);


            // show all grid items
            $('.loading').removeClass('loading');
        }

        function setMobileParallax() {
            setInterval(function() {
              var windowCenter = w.scrollTop(); 
              var windowScrollTop = w.scrollTop();

                $('.scroll-div-mobile').each(function() {
                var container = $(this);
                var sectionCenter = container.offset().top;
                var sectionDist = sectionCenter - windowCenter;
               
                container.find('.grid-item-mobile').each(function() {
                  var offset = parseInt($(this).data('offset'));
                  var speed = parseInt($(this).data('speed'));
                  var targetTransY = (sectionDist * offset)/10;
                  var currentTransY = parseInt($(this).css('transform').split(',')[5]);
                  var transYDif = targetTransY - currentTransY;
                  $(this).css({'transform' : 'translateY(' + (currentTransY + (transYDif/speed)) + 'px)'});
                });
              });
            }, 40);
        }

        function killAnchorItems() {
            $('.bloomies-anchor').off();
            $('.bloomies-anchor-top').off();
        }

        function setAnchorItemsDesktop() {
            killAnchorItems();

            $('.bloomies-anchor').on('click', function(e) {
                e.preventDefault();
                scrollTo($(this).attr('href'));
            });

            $('.bloomies-anchor-top').on('click', function(e) {
                e.preventDefault();
                $("html").animate({ scrollTop: 0 }, pageScrollSpeed);
            });
        }

        function setAnchorItemsMobile() {
            killAnchorItems();

            $('.bloomies-anchor').on('click', function(e) {
                e.preventDefault();
                var targetEl = $(this).attr('href') + '-mobile';
                $("html").animate({ scrollTop: $(targetEl).offset().top }, pageScrollSpeed);
            });

            $('.bloomies-anchor-top').on('click', function(e) {
                e.preventDefault();
                $("html").animate({ scrollTop: 0 }, pageScrollSpeed);
            });
        }

        function setBgFade() {
            colorChangeOnScroll();

            // set the speed for the background fade on load
            document.body.style.setProperty('--bg-speed', '1s');
        }

        function showVideoBackups() {
            setTimeout(function() { $('.video-bg').css('opacity', 1); }, 1000);
        }

        function setDesktop() {
            desktopImagesLoaded = true;
            $('img').each(function() {
                if($(this).data('desktop-src')) {
                    $(this).attr('src', $(this).data('desktop-src'));
                }
            });
            
            initDesktopCarousel();
            initScroll();
            setAnchorItemsDesktop();
        }

        function initDesktopCarousel() {
            var carousel = $('#fresh-looks-carousel .looks-carousel');
            if(!carousel.hasClass('slick-initialized')) {
                carousel.slick({
                    infinite: true,
                    slidesToShow: 1,
                    dots: false,
                    speed: 300,
                    autoplay: true,
                    autoplaySpeed: 4000,
                });

                carousel.on('afterChange', function(event, slick, currentSlide, nextSlide){
                    $('.fresh-clouds-video').each(function() {
                        $(this).find('video')[0].pause();
                        $(this).find('video').attr('currentTime', 0);
                        $(this).find('video')[0].play();
                    });
                });
            }
        }

        function initMobileCarousel() {
            var carousel = $('.fresh-looks-mobile-slider');
            if(!carousel.hasClass('slick-initialized')) {
                carousel.slick({
                    infinite: true,
                    slidesToShow: 1,
                    dots: false,
                    arrows: false,
                    speed: 300,
                    autoplay: true,
                    autoplaySpeed: 6000,
                });

                carousel.on('afterChange', function(event, slick, currentSlide, nextSlide){
                    $('.fresh-clouds-video-mobile-vid').each(function() {
                        $(this).find('video')[0].pause();
                        $(this).find('video').attr('currentTime', 0);
                        $(this).find('video')[0].play();
                    });
                });
            }
        }

        function setMobile() {
            initMobileCarousel();
            setAnchorItemsMobile();
            setMobileParallax();
        }

        function setSitePosition() {
            var mainPos = ($(window).height()/2) - ($('.logo-panel').height()*.6);
            $('#main').css('transform', 'translateY(' + mainPos + 'px)');
        }

        function checkIntroSize() {
            // set the intro video container to 100% width and height
            $('.intro-video-container').width('100%').height('100%');

            // get video element
            var video = $('#intro-video').find('video');

            // set video element width and height
            if(w.height() > w.width()) {
                video.width('auto');
                video.height('101vh');
            }
            else {
                video.height('auto');
                video.width('101vw');
            }

            setSitePosition();
        }
        
        // load all videos
        function loadAllVideos() {
            $('.media-player-div').each(function(index, element) {
                var el = $(this);
                var videoId = el.data('video-id');
                getBrightcoveVideoData(videoId, el, function(data) {
                    videoDataLoaded(data, el);
                });
            });
        }

        function getBrightcoveVideoData(videoId, element, callback) {
            var path = `https://edge.api.brightcove.com/playback/v1/accounts/${accountID}/videos/${videoId}`;
            $.ajax(
                {
                    type: 'GET',
                    url: path,
                    cache: false,
                    contentType: 'application/json',
                    crossDomain: true,
                    processData: true,
                    headers: {
                    Accept: `application/json;pk=${policyKey}`,
                },

                success(brightcoveVideoData) {
                    callback(brightcoveVideoData);
                },

                error(xhr, status, errorThrown) {
                    if(element.hasClass('bloomies-intro-video')) {
                        introLoadFail();
                    }
                },
            });
        }

        function videoDataLoaded(data, element) {
            var videoSource = getVideoSource(data);
            var videoEl = $(element);
            var videoId = videoEl.data('video-id');
            var autoplayinline = '';
            var playsinline = '';
            var loopinline = '';
            var mutedinline = '';

            if (videoEl.data('autoplay') === true) {
                autoplayinline = 'autoplay';
                mutedinline = 'muted';
                playsinline = 'playsinline';
            }
            mutedinline = 'muted';

            if (videoEl.data('loop') === true) {
                loopinline = 'loop';
            }

            // build video
            var id ="";

            if(videoEl.hasClass('say-hello-video')) {
                id = "say-hello-video-video";
            }
            videoEl.append(`<video id="${id}" crossorigin ${autoplayinline} ${playsinline} ${loopinline} ${mutedinline} id="vid-${videoId}" class="media-player" data-name="${videoEl.data('name')}" data-video="${data.id}">
            <source src="${videoSource}" type="video/mp4">
            </video>`);

            // if this is the video play it
            if(videoEl.hasClass('bloomies-intro-video')) {
                showIntro();
            }

            if(videoEl.hasClass('say-hello-video')) {
                $('#say-hello-video-video').on('loadstart', function() {
                    setSeeYouSoon();
                });
            }
        }

        function getVideoSource(data) {
            var videoSourceArray = [];
            for (var i = 0; i < data.sources.length; i += 1) {
                if ('src' in data.sources[i]) {
                    var srcToCheck = data.sources[i].src;
                    if (srcToCheck.startsWith('https') && srcToCheck.indexOf('mp4')) {
                        videoSourceArray.push(data.sources[i]);
                    }
                }
            }

            var maxWidth = 0;
            var maxWidthIndex;
            for (var i = 0; i < videoSourceArray.length; i += 1) {
                if (videoSourceArray[i].width > maxWidth) {
                    maxWidth = videoSourceArray[i].width;
                    maxWidthIndex = i;
                }
            }
            return videoSourceArray[maxWidthIndex].src;
        }

        function introLoadFail() {
            init();
            introFinished();
        }

        function showIntro() {
            w.on('resize', checkIntroSize.intro);
            checkIntroSize();
            init();

            var introVideo = $('#intro-video').find('video');
            introVideo.on('ended',function(){
                introFinished();
            });
        }

        function introFinished() {
            // stop window listener
            w.off('.intro');

            $('.intro-video-container').addClass('intro-finished');
            $('.intro-video-container').animate({'opacity': 0}, 500, function() {
                $('.intro-video-container').remove();
                setStickyNav();
                showSite();
            });
        }

        function showSite() {
            $('#main').attr('style', '--transition-speed: 2s');
            setTimeout(function() {
                $('.logo-tagline').addClass('animate');
            }, 200);

            setTimeout(function() {
                $('#main').css('transform', 'translateY(0px)');
            }, 2200);

            setTimeout(function() {
                $('.icons img').each(function() {
                    $(this).addClass('animate');
                });
            }, 2300);

            setTimeout(function() {
                $('.bloomies-store-panel').addClass('animate');
                $('body').addClass('intro-finished');
            }, 3000);

            setTimeout(function() {
                $('.intro-panel-top').addClass('animate');
                w.scroll();
            }, 2500);

            setTimeout(function() {
                $('.intro-text-bottom').addClass('animate');
            }, 3900);
        }

        function loadIntroVideo() {
            var el;

            // set cookie not to show animation next time
            setCookie('bloomiesvisit', 1, 30);

            // set the site position
            setSitePosition();

            // get intro container based on window width
            (w.width() < 600) ? el = $('#intro-video-mobile') : el = $('#intro-video-desktop');

            // get video id based on mobile or desktop
            var videoId = el.data('video-id');

            // load brightcove video
            getBrightcoveVideoData(videoId, el, function(data) {
                videoDataLoaded(data, el);
            });
        }

        function skipIntro() {
            init();
            introFinished();
        }

        function getCookie(cname) {
          let name = cname + "=";
          let decodedCookie = decodeURIComponent(document.cookie);
          let ca = decodedCookie.split(';');
          for(let i = 0; i <ca.length; i++) {
            let c = ca[i];
            while (c.charAt(0) == ' ') {
              c = c.substring(1);
            }
            if (c.indexOf(name) == 0) {
              return c.substring(name.length, c.length);
            }
          }
          return "";
        }

        function setCookie(cname, cvalue, exdays) {
          const d = new Date();
          d.setTime(d.getTime() + (exdays*24*60*60*1000));
          let expires = "expires="+ d.toUTCString();
          document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
        }

        function checkCookies() {
            getCookie('bloomiesvisit') === '' ? loadIntroVideo() : skipIntro();
        }

        function setIntialScroll() {
            window.scroll(0, 0); 
            if (history.scrollRestoration) {
                history.scrollRestoration = 'manual';
            }
        }

        function init() {
            setBgFade();
            listenForWindowResize();
            showVideoBackups();
            initLightboxMenu();
            //loadAllVideos();
        }

        setIntialScroll();
        checkCookies();

    });


</script>

