        function colorChangeOnScroll() {
            w.scroll(function() {
                // selectors
                var panel = $('.panel');
                var nav = $('.sticky-nav');

                // get scroll position
                var scroll = w.scrollTop() + (w.height() / 2);

                // loop through panels
                panel.each(function () {
                    var $this = $(this);
                    if ($this.position().top <= scroll && $this.position().top + $this.height() > scroll) {
                        
                        // set classes on body
                        body.removeClass(function (index, css) {
                            return (css.match (/(^|\s)color-\S+/g) || []).join(' ');
                        });
                        body.addClass('color-' + $(this).data('color'));

                        // set classes on nav
                        nav.removeClass(function (index, css) {
                            return (css.match (/(^|\s)color-\S+/g) || []).join(' ');
                        });
                        nav.addClass('color-' + $(this).data('color'));

                        $('.use-bg-color').removeClass(function (index, css) {
                            return (css.match (/(^|\s)color-\S+/g) || []).join(' ');
                        });
                        $('.use-bg-color').addClass('color-' + $(this).data('color'));
                    }
                });    

            });
        }

        function getScrollSpeed(dist) {
            var speed = dist * 1.4;
            if(speed < 1500) {
                speed = 1500;
            }
            return speed;
        }

        function scrollTo(el) {
            var pos = ($(el).offset().top + (($(el).height()/2) - $(window).height()/2));
            if(($(el).height() + $('.sticky-nav-outer').height()) > $(window).height()) {
                pos = pos - 45;
            }
            var dist = Math.abs(pos - $('html').scrollTop());
            $('html').animate({ scrollTop: pos }, getScrollSpeed(dist));
        }

